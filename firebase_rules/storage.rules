rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // allow read access for public images
    match /products/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }

    // default deny
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
